<!DOCTYPE html>
<html>
    <head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">

		<title>{% block title '' %}</title>

		{% include 'layout/inspinia/main_css.html.twig' only %}
		{% include 'layout/app/main_css.html.twig' only %}

		<script src="{{ asset('inspinia/js/jquery-2.1.1.js') }}"></script>
		<script src="{{ asset('inspinia/js/jquery-ui-1.10.4.min.js') }}"></script>
    </head>

    {% block body %}
        <body>
            <div id="modals"></div>

            <div id="wrapper">
                {% block navigation %}
                    {% include 'layout/app/navigation.html.twig' only %}
                {% endblock %}
                <div id="page-wrapper" class="gray-bg">
                    {% block navbar %}
                        {% include 'layout/app/navbar.html.twig' only %}
                    {% endblock %}
                    {% block nav '' %}
                    {% block content '' %}
                    {% block footer %}
                        {% include 'layout/app/footer.html.twig' only %}
                    {% endblock %}
                </div>
            </div>

            {% include 'layout/inspinia/main_js.html.twig' only %}
            {% include 'layout/app/main_js.html.twig' only %}
        </body>
    {% endblock %}
</html>
